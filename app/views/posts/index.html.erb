<h1>Articles</h1>
  <%= link_to 'New Post', new_post_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th colspan="2"></th>
  </tr>

  <% @posts.each do |post| %>
    <tr>
      <td> <%= post.title %> </td>
      <td> <%= post.content%> </td>
      <td> <%= link_to 'Show', post_path(post) %> </td>
      <td> <%= link_to 'Edit', edit_post_path(post) %> </td>
    </tr>
  <% end %>
</table>